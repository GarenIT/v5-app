<view id="guide"><swiper class="swiper" style="{{'height:'+(screenHeight+'px')+';'}}" circular indicator-dots="{{true}}" current="{{tabIndex}}" data-event-opts="{{[['change',[['changeTab',['$event']]]]]}}" bindchange="__e"><block wx:for="{{initlList.init}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['jumpAdUrl',['$0'],[[['initlList.init','',index,'url']]]]]]]}}" class="item" bindtap="__e"><image src="{{item.src}}" mode="aspectFill"></image><block wx:if="{{initTime!=0}}"><view class="stopTime">{{initTime+" s"}}</view></block><block wx:if="{{index==endId}}"><view data-event-opts="{{[['tap',[['jumpIndex',['$event']]]]]}}" class="experUrl" bindtap="__e">马上体验</view></block></swiper-item></block></swiper></view>