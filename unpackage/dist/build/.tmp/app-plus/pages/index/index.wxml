<view class="content"><view class="head-content"><view class="head-placeholder"></view><view class="head-search"><image class="head-logo" src="{{homeData.app_logo}}" mode="aspectFill"></image><view class="head-search-center"><input class="head-input" type="text" maxlength="20" placeholder="精彩内容等你来搜索" data-event-opts="{{[['input',[['searchKey',['$event']]]]]}}" bindinput="__e"/><image class="head-search-img" src="../../static/imgs/search.png" mode="aspectFill"></image></view><image class="head-history" src="../../static/imgs/history.png" mode="aspectFill" data-event-opts="{{[['tap',[['watchLog',['$event']]]]]}}" bindtap="__e"></image></view><view class="head-class"><scroll-view class="class-scroll" scroll-x="true"><view data-event-opts="{{[['tap',[['subClassSel',['$0',0],['className']]]]]}}" class="{{[classId==0?'head-class-sel':'']}}" bindtap="__e">全部</view><block wx:for="{{homeClass.homeTab}}" wx:for-item="c" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['subClassSel',['$0','$1'],['className',[['homeClass.homeTab','',index,'tabId']]]]]]]}}" class="{{[classId==c.tabId?'head-class-sel':'']}}" bindtap="__e">{{''+c.tabName+''}}</view></block></scroll-view></view></view><view class="line-left-right"><image class="line-left" src="../../static/imgs/left.png" mode="aspectFill"></image><image class="line-right" src="../../static/imgs/right.png" mode="aspectFill"></image></view><view class="head-banner"><swiper class="swiper" circular interval="3000" autoplay="{{true}}" indicator-dots="{{true}}" indicator-color="rgba(255,108,52,0.8)" indicator-active-color="#ffffff"><block wx:for="{{homeData.banner}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item data-event-opts="{{[['tap',[['jumpAdUrl',['$0'],[[['homeData.banner','',index,'url']]]]]]]}}" class="item" bindtap="__e"><image src="{{item.images_url}}" mode="aspectFill"></image><view class="banner-intro">{{item.info}}</view></swiper-item></block></swiper></view><view class="head-notice"><swiper class="head-notice-content" vertical="true" autoplay="false" duration="1000" interval="4000"><block wx:for="{{notice}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view data-event-opts="{{[['tap',[['lookNotice',['$0'],[[['notice','',index]]]]]]]}}" bindtap="__e"><image class="noticeImg" src="../../static/imgs/notice.png" mode="aspectFill"></image><view class="head-notice-title">{{item.title}}</view><view class="head-notice-time">{{item.add_date}}</view></view></swiper-item></block></swiper></view><neil-modal vue-id="1" show="{{isNotice}}" show-cancel="{{false}}" title="{{noticeTitle}}" content="{{noticeContent}}" data-event-opts="{{[['^close',[['closeNotice']]]]}}" bind:close="__e" bind:__l="__l"></neil-modal><view class="{{[isFlex?'class-list class-list-position':'class-list']}}"><block wx:for="{{classData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{item.type!=className}}"><view class="sub-class"><view class="class-type">{{item.name}}</view><scroll-view class="subclass-scroll" scroll-x="true"><block wx:for="{{item.items}}" wx:for-item="c" wx:for-index="cindex" wx:key="cindex"><view data-event-opts="{{[['tap',[['subClassSel',['$0','$1'],[[['classData','',index,'type']],[['classData','',index],['items','',cindex,'id']]]]]]]}}" class="{{[tagId==c.id&&tagName==item.type||areaId==c.id&&areaName==item.type?'class-sel':'']}}" bindtap="__e">{{''+c.name+''}}</view></block></scroll-view></view></block></block><block wx:if="{{!isSubClassShow}}"><view class="sub-class"><view class="class-type">排序</view><scroll-view class="subclass-scroll" scroll-x="true"><view data-event-opts="{{[['tap',[['subClassSel',['orderCode','lastTime']]]]]}}" class="{{[orderCode=='lastTime'?'class-sel':'']}}" bindtap="__e">最新</view><view data-event-opts="{{[['tap',[['subClassSel',['orderCode','hot']]]]]}}" class="{{[orderCode=='hot'?'class-sel':'']}}" bindtap="__e">热门</view><view data-event-opts="{{[['tap',[['subClassSel',['orderCode','reco']]]]]}}" class="{{[orderCode=='reco'?'class-sel':'']}}" bindtap="__e">推荐</view><view data-event-opts="{{[['tap',[['subClassSel',['orderCode','good']]]]]}}" class="{{[orderCode=='good'?'class-sel':'']}}" bindtap="__e">点赞</view></scroll-view></view></block></view><view class="{{[isAddHeight?'video-list class-list-add-height':'video-list']}}"><block wx:for="{{videoData}}" wx:for-item="i" wx:for-index="k" wx:key="k"><view data-event-opts="{{[['tap',[['playVideo',['$0'],[[['videoData','',k,'id']]]]]]]}}" class="video-info" bindtap="__e"><image class="videoImg" src="{{i.thumbnail}}" mode="aspectFill"></image><view class="video-playTime">{{i.play_time}}</view><view class="video-title">{{i.title}}</view><view class="video-gold"><image src="../../static/imgs/gold.png" mode="aspectFill"></image>{{''+i.gold}}</view><view class="video-date">{{i.update_date}}</view></view></block></view><block wx:if="{{isShowTig}}"><view style="width:100%;height:500rpx;margin-top:100rpx;text-align:center;"><image style="width:280rpx;height:180rpx;" src="../../static/imgs/nodata.png"></image><view style="width:100%;font-size:25rpx;color:#666;margin-top:30rpx;">暂无视频，请稍后再试</view></view></block><block wx:if="{{isMoreTig}}"><view class="moreTig">------------------我是有底线的------------------</view></block></view>