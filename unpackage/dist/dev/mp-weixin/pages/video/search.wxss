.search-log-content view{
	display: inline-block;
}
.search-log-list{
	width: 25%;
	height: 30px;
	line-height: 30px;
	text-align: center;
	background: #f5f5f5;
	font-size: 13px;
	-webkit-border-radius: 3px;
	        border-radius: 3px;
	color: #999999;
	margin: 0 0 5px 10px;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	padding: 0 5px;
}
.search-log{
	margin-top: 10px;
	padding: 0 35rpx;
	font-size: 17px;
	color: #666666;
	font-weight: bold;
	/*background: #007AFF;
 */
}
.search-log-top{
	/*height: 30px;
 */
	line-height: 20px;
	/*background: #007AFF;
 */
}
.search-log-top image{
	width: 20px;
	height: 20px;
	float: right;
}
.content{
	margin-top: 18px;
}
.head-content{
	width: 94%;
	background: #f8f8f8;
	/*background: -webkit-gradient(linear, left top, right top, from(#ff8566), to(#ff6f33));   
 */
	padding: 0rpx 25rpx 10rpx;
	text-align: center;
	position: fixed;
	top: 0;
	z-index: 999;
	border-bottom: 1px solid #f3f3f3;
}
.head-placeholder{



	padding-top: 16rpx;
	-webkit-box-sizing: content-box;
	        box-sizing: content-box;
}
.head-left{
	width: 10%;
	display: inline-block;
	float: left;
}
.head-left image{
	width: 27px;
	height: 27px;
	margin-top: 3px;
	/*background: rgba(0,0,0,0.5);
 */
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	opacity: 0.6;
	filter: alpha(opacity=60);
}
.head-right{
	width: 10%;
	/*background: #007AFF;
 */
	display: inline-block;
	float: right;
	text-align: left;
}
.head-right image{
	width: 32px;
	height: 32px;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
}
.head-search-center{




	width: 83%;

	display: inline-block;
	/*background-color: #007AFF;
 */
}
.head-search .head-input{
	height: 32px;
	line-height: 32px;
	background: #E5E5E5;
	-webkit-border-radius: 50px;
	        border-radius: 50px;
	font-size: 26rpx;
	padding: 0px 30rpx;
	text-align: left;
	color: #666666;
}
