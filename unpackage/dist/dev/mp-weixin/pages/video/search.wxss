	float: left;
}
.video-info image{
	width: 100%;
	height: 200rpx;
	-webkit-border-radius: 5px 5px 0 0;
	        border-radius: 5px 5px 0 0;
}
.video-info .title{
	font-size: 12px;
	/*color: #666666;
 */
	line-height: 30rpx;
	background: #007AFF;
	/*padding: 0 3px;
 */
}
.one{
	background: #B01A1C;
	color: #FFFFFF!important;
}
.two{
	background: #007AFF;
	color: #FFFFFF!important;
}
.thr{
	background: #A83FDB;
	color: #FFFFFF!important;
}
.search-hot-list{
	width: 92%;
	/* height: 30px;
	line-height: 30px; */
	font-size: 13px;
	-webkit-border-radius: 3px;
	        border-radius: 3px;
	margin: 0 5% 0 3%;
}
.search-hot-sort{
	width: 20px;
	height: 20px;
	line-height: 20px;
	display: inline-block;
	text-align: center;
	/*background: #007AFF;
 */
	-webkit-border-radius: 50%;
	        border-radius: 50%;
	color: #666666;
}
.search-hot-title{
	width: 63%;
	padding: 0 5% 0 0;
	height: 20px;
	line-height: 20px;
	display: inline-block;
	/*background: #1ABD6C;
 */
	color: #777777;
	float: right;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.search-hot-time{
	width: 22%;
	/*background: #007AFF;
 */
	float: right;
	height: 20px;
	line-height: 20px;
	text-align: right;
}
.search-hot-content{
	margin-top: 10px;
