.video-desc{
	width: 100%;
	text-align: center;
	font-size: 12px;
	color: #666666;
	margin: 10px 0;
}
.video-info{
	width: 45%;
	height: 265rpx;
	/*background: #1AAD19;
 */
	-webkit-border-radius: 5px;
	        border-radius: 5px;
	border: 1px solid #E5E5E5;
	padding-bottom: 8px;
