<view><view class="head-content" id="head"><view class="head-placeholder"></view><view class="head-search"><view class="head-search-center"><input class="head-input" focus="true" confirm-type="search" type="search" maxlength="20" placeholder="搜索一下,发现新世界..." data-event-opts="{{[['input',[['inputKey',['$event']]]],['confirm',[['searchKey',[1]]]]]}}" value="{{search}}" bindinput="__e" bindconfirm="__e"/></view><view data-event-opts="{{[['tap',[['searchKey',[1]]]]]}}" class="head-right" bindtap="__e"><image src="/static/imgs/search.png" mode="aspectFill"></image></view></view></view><view style="{{'height:'+(topHeight+'px')+';'}}"></view><block wx:if="{{isShearchShow}}"><view class="content"><block wx:if="{{logHide}}"><view class="search-log"><view class="search-log-top"><text>历史搜索</text><image src="/static/imgs/del.png" data-event-opts="{{[['tap',[['delSearch',['$event']]]]]}}" bindtap="__e"></image></view><view class="search-log-content"><block wx:for="{{logList}}" wx:for-item="t" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['searchLog',['$0'],[[['logList','',i,'content']]]]]]]}}" class="search-log-list" bindtap="__e"><text>{{t.content}}</text></view></block></view></view></block><block wx:if="{{hotHide}}"><view class="search-log"><view class="search-log-top hot"><text>热门搜索</text></view><block wx:for="{{$root.l0}}" wx:for-item="e" wx:for-index="h" wx:key="h"><view data-event-opts="{{[['tap',[['playVideo',['$0'],[[['hotList','',h,'id']]]]]]]}}" class="search-hot-content" bindtap="__e"><view class="search-hot-list"><block wx:if="{{h+1==1}}"><text class="search-hot-sort one">{{h+1}}</text></block><block wx:if="{{h+1==2}}"><text class="search-hot-sort two">{{h+1}}</text></block><block wx:if="{{h+1==3}}"><text class="search-hot-sort thr">{{h+1}}</text></block><block wx:if="{{h+1>3}}"><text class="search-hot-sort">{{h+1}}</text></block><text class="search-hot-time">{{e.g0}}</text><text class="search-hot-title">{{e.$orig.title}}</text></view></view></block></view></block></view></block><block wx:else><view class="video-list"><view class="video-desc">{{"共搜索到"+videoCount+"个与"}}<text class="colorRed">{{search}}</text>相关的视频</view><block wx:for="{{videoList}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view data-event-opts="{{[['tap',[['playVideo',['$0'],[[['videoList','',k,'id']]]]]]]}}" class="video-info" bindtap="__e"><image src="{{v.thumbnail}}" mode="aspectFill"></image><view style="padding:0 3px;"><rich-text class="title" nodes="{{v.title}}">...</rich-text></view></view></block><block wx:if="{{isMoreTig}}"><view class="moreTig">{{isMoreMsg}}</view></block></view></block></view>