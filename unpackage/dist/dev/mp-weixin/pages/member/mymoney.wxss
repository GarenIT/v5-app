.head{ 
	width: 100%;
	height: 280px;
	background-image: -webkit-gradient(linear, left top, right top, from(rgba(255,12,208,0.8)), to(rgba(254,67,101,0.8)));
	background-image: -webkit-linear-gradient(left, rgba(255,12,208,0.8), rgba(254,67,101,0.8));
	background-image: linear-gradient(to right, rgba(255,12,208,0.8), rgba(254,67,101,0.8));
	text-align: center; 
	-webkit-border-radius: 0 0 50rpx 50rpx; 
	        border-radius: 0 0 50rpx 50rpx;
}
.head view{ display:inline-block;width:50%;color:#FFFFFF;
}
.head-money{ height:250px;
}
.head-money view{ display:block;width:100%;font-size:20px;
}
.head-money image{ width:50px;height:50px;margin-top:90px;
}
.head-money .head-wz{ color:rgba(255,255,255,0.7);
}
.content{ 
	width: 90%;
	-webkit-border-radius: 10px;
	        border-radius: 10px;
	background: #FFFFFF;margin: -50px auto 0;
	-webkit-box-shadow: 0px 4px 6px 0px rgba(0, 0, 0, 0.5);
	        box-shadow: 0px 4px 6px 0px rgba(0, 0, 0, 0.5);
	padding: 0 0 25px;
}
.content view{ display: inline-block;
}
.content-gt{ width: 35%;margin: 20px 0 0 10%;text-align: center;padding: 10px 0;-webkit-border-radius: 10px;border-radius: 10px;color: #666666;
}
.content-gt image{ width: 40px;height: 40px;
}
.content-gt view{ display: block;font-size: 15px;
}

