.mask {
	/* pointer-events: none; */
	position: fixed;
	z-index: 99999;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	height: 100vh;
	width: 100vw;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient:vertical;
	-webkit-box-direction:normal;
	-webkit-flex-direction:column;
	        flex-direction:column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap; 
	margin-top: -50rpx;
}
.mask .title{
	color: #ffffff;
	font-size: 12px;
	margin-top: 40rpx;
}
.mask.mask-show{
	background: rgba(7, 17, 27, .3);
}
/* loading加载动画的css */
.loading13 {
  width: 45rpx;
  position: relative;
}
.loading13 view {
  display: block;
  position: absolute;
  bottom: 0rpx;
  width: 9rpx;
  height: 5rpx;
  background: coral;
  -webkit-animation: loading13 1.5s infinite ease-in-out;
  animation: loading13 1.5s infinite ease-in-out;
}
.loading13 view:nth-child(2) {
  left: 11rpx;
  -webkit-animation-delay: 0.2s;
  animation-delay: 0.2s;
}
.loading13 view:nth-child(3) {
  left: 22rpx;
  -webkit-animation-delay: 0.4s;
  animation-delay: 0.4s;
}
.loading13 view:nth-child(4) {
  left: 33rpx;
  -webkit-animation-delay: 0.6s;
  animation-delay: 0.6s;
}
.loading13 view:nth-child(5) {
  left: 44rpx;
  -webkit-animation-delay: 0.8s;
  animation-delay: 0.8s;
}
@-webkit-keyframes loading13 {
0% {
    height: 5rpx;
    -webkit-transform: translateY(0rpx);
    transform: translateY(0rpx);
    background: coral;
}
25% {
    height: 30rpx;
    -webkit-transform: translateY(15rpx);
    transform: translateY(15rpx);
    background: cornflowerblue;
}
50% {
    height: 5rpx;
    -webkit-transform: translateY(0rpx);
    transform: translateY(0rpx);
    background: cornflowerblue;
}
100% {
    height: 5rpx;
    -webkit-transform: translateY(0rpx);
    transform: translateY(0rpx);
    background: coral;
}
}
@keyframes loading13 {
0% {
    height: 5rpx;
    -webkit-transform: translateY(0rpx);
    transform: translateY(0rpx);
    background: coral;
}
25% {
    height: 30rpx;
    -webkit-transform: translateY(15rpx);
    transform: translateY(15rpx);
    background: cornflowerblue;
}
50% {
    height: 5rpx;
    -webkit-transform: translateY(0rpx);
    transform: translateY(0rpx);
    background: cornflowerblue;
}
100% {
    height: 5rpx;
    -webkit-transform: translateY(0rpx);
    transform: translateY(0rpx);
    background: coral;
}
